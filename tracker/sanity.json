{
  "root": true,
  "project": {
    "name": "HeySugar (Gatsby)"
  },
  "api": {
    "projectId": "c9o3u1jm",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@hey-sugar/sanity-plugin-hey-sugar-schema"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./src/structure/deskStructure.js"
    }
  ]
}